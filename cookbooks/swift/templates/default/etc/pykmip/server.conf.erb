[server]
hostname=127.0.0.1
port=5696
certificate_path=<%= @server_crt %>
key_path=<%= @server_key %>
ca_path=<%= @ca_crt %>
auth_suite=Basic
policy_path=/etc/pykmip/policies
enable_tls_client_auth=False
tls_cipher_suites=
    TLS_RSA_WITH_AES_128_CBC_SHA256
    TLS_RSA_WITH_AES_256_CBC_SHA256
    TLS_ECDHE_ECDSA_WITH_AES_256_CBC_SHA384
logging_level=DEBUG
