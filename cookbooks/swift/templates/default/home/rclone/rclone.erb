[admin]
type = swift
env_auth = false
user = admin:admin
key = admin
auth = <%= @auth_uri %>
auth_version = 1

[admin-s3]
type = s3
env_auth = false
access_key_id = admin:admin
secret_access_key = admin
region = us-east-1
endpoint = <%= @base_uri %>

